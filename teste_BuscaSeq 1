#include <stdio.h>
#include <stdlib.h>
#include <string.h>
//#define TAM 5

int buscaSeq(int *vet, char convidado){
    int i, tamanho;
    
    for(i=0; i<tamanho; i++){
        if(convidado == vet[i]){
        return (1);
        }
    }
   return (0); 
}


int main() {
    int i, v, tamanho;
   // int busca;
    char elemento[tamanho][250];
    
    buscaSeq(&v, elemento);
    
    printf("\nInforme a quantidade mÃ¡xima de convidados: ");
    scanf("%d", &tamanho);
    
    v = strlen(elemento);
    
    
    
    for(i=0; i<tamanho; i++){
        printf("\nNome do convidado %d: ", i+1);
        scanf("%s", elemento[i]);
    }
    
    

    return 0;
}
